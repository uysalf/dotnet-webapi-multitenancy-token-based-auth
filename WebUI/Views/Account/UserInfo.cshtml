

@model UserModel

@{
    ViewData["Title"] = "User Info";
}

<h1>@ViewData["Title"]</h1>



<div class="container">
    <div class="form-group row">
        <label asp-for="Id" class="col-sm-2 col-form-label"></label>
        <label asp-for="Id" class="col-sm-10 col-form-label">@Model.Id</label>
    </div>
    <div class="form-group row">
        <label asp-for="UserName" class="col-sm-2 col-form-label"></label>
        <label asp-for="UserName" class="col-sm-10 col-form-label">@Model.UserName</label>
    </div>
    <div class="form-group row">
        <label asp-for="FirstName" class="col-sm-2 col-form-label"></label>
        <label asp-for="FirstName" class="col-sm-10 col-form-label">@Model.FirstName</label>
    </div>
    <div class="form-group row">
        <label asp-for="LastName" class="col-sm-2 col-form-label"></label>
        <label asp-for="LastName" class="col-sm-10 col-form-label">@Model.LastName</label>
    </div>
    <div class="form-group row">
        <label asp-for="Email" class="col-sm-2 col-form-label"></label>
        <label asp-for="Email" class="col-sm-10 col-form-label">@Model.Email</label>
    </div>

    <div class="form-group row">
        <label asp-for="SelectedRoles" class="col-sm-2 col-form-label"></label>
        <label asp-for="SelectedRoles" class="col-sm-10 col-form-label">@Model.SelectedRoles</label>
    </div>
</div>